 "prepared_report": 0,
 "query": "SELECT \n    sl.posting_date AS \"Ng\u00e0y, th\u00e1ng\", \n    CASE WHEN sl.actual_qty > 0 THEN sl.voucher_no ELSE NULL END AS \"voucher_no_input\",\n    CASE WHEN sl.actual_qty < 0 THEN sl.voucher_no ELSE NULL END AS \"voucher_no_output\",\n    NULL AS \"Di\u1ec5n gi\u1ea3i\",\n    sl.posting_date AS \"Ng\u00e0y nh\u1eadp, xu\u1ea5t\",\n    GREATEST(sl.actual_qty, 0) AS \"Nh\u1eadp\",\n    ABS(LEAST(sl.actual_qty, 0)) AS \"Xu\u1ea5t\",\n    sl.qty_after_transaction AS \"T\u1ed3n\"\nFROM \n    `tabStock Ledger Entry` sl \n    LEFT JOIN `tabItem` it ON sl.item_code = it.item_code \nWHERE \n    POSTING_DATE BETWEEN DATE(%(start_date)s) AND DATE(%(end_date)s)\n    AND COMPANY = %(company)s AND WAREHOUSE = %(warehouse)s\n    AND it.item_code = %(item_name)s",
 "ref_doctype": "Stock Entry",
